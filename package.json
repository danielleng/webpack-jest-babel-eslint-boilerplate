{
  "name": "webpack-jest-babel-eslint-boilerplate",
  "version": "1.0.0",
  "description": "A front-end web project boilerplate utilizing the Webpack bundler configured with Jest, ESLint (Airbnb code style) and Babel.",
  "keywords": [
    "boilerplate",
    "webpack boilerplate",
    "webpack jest",
    "webpack eslint",
    "webpack babel"
  ],
  "main": "webpack.config.js",
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --open",
    "watch": "webpack-dev-server --open",
    "test": "jest --config .jestrc.json",
    "test-watch": "jest --config .jestrc.json --watchAll"
  },
  "author": {
    "name": "Daniel Leng",
    "email": "projects@dleng.xyz",
    "url": "http://dleng.xyz/"
  },
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "eslint": "^4.12.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
